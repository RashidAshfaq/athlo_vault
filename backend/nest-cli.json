{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "projects": {
    "admin": {
      "type": "application",
      "entryFile": "apps/admin/src/main",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "athlete": {
      "type": "application",
      "entryFile": "apps/athlete/src/main",
      "compilerOptions": {
        "tsConfigPath": "apps/athlete/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "entryFile": "apps/auth/src/main",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "entryFile": "libs/common/src/index",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "fan": {
      "type": "application",
      "entryFile": "apps/fan/src/main",
      "compilerOptions": {
        "tsConfigPath": "apps/fan/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "entryFile": "apps/gateway/src/main",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "investor": {
      "type": "application",
      "entryFile": "apps/investor/src/main",
      "compilerOptions": {
        "tsConfigPath": "apps/investor/tsconfig.app.json"
      }
    }
  },
  "monorepo": true
}